{
  "name": "proof-of-address",
  "version": "1.0.0",
  "description": "Smart contract and DApp by poa.network to create proof of a physical address",
  "license": "MIT",
  "main": "server.js",
  "scripts": {
    "build": "npm run build --prefix web-dapp",
    "coverage": "npm run coverage --prefix web-dapp",
